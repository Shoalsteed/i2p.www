msgid ""
"Data <a href=\"%(cryptography)s#AES\">AES encrypted</a> using the DH "
"session key and\n"
"                  the last 16 bytes of the encrypted contents of message "
"#2 as the IV\n"
"                  48 bytes for a DSA signature, may vary for other "
"signature types"
msgstr ""

